rsync -avz --exclude-from 'exclude-list.txt' shed/ pi@192.168.0.23:/var/www/html/shed/
rsync -avz --chmod=Fu=rwx,Fg=rwx,Fo=rwx shed.conf pi@192.168.0.23:/etc/apache2/sites-available/shed.conf
rsync -avz --chmod=Fu=rwx,Fg=rwx,Fo=rwx requirements.txt pi@192.168.0.23:/var/www/html/shed/requirements.txt
ssh pi@192.168.0.23 sudo service apache2 restart
